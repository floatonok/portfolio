<div id="about-me" style="background: url(background.jpg); background-size: cover;">
  <div class="container-fluid">
    <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1"></div>
    <div class="col-lg-5 col-md-5 col-sm-5 col-xs-5" style="padding: 0 50px 0 50px;">
      <object id="Layer_1" type="image/svg+xml" data="logo1.svg" style="height: 400px;"></object>
    </div>
    <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1"></div>
    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 wow bounceInUp" style="margin-top: 450px; text-align: justify; word-spacing:-2px;" data-wow-duration="2s" data-wow-delay="-0.5s">
      <ul>
        {{#each profiles as |profile|}}
          <h3>
            {{profile.currently}}
          </h3>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
