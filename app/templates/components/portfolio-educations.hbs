<div id="educations" style="background-image:url(line.svg); background-position:center; background-repeat: no-repeat;">
  <div class="container-fluid" style="margin-top: 10%;">
    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2"></div>
    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3" style="text-align: right;">
      <h2>EDUCATION</h2>
      <ul>
        {{#each educations as |education|}}
          <li>
            {{education.school}} | {{education.duration}}
          </li>
        {{/each}}
      </ul>
    </div>
    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2"></div>
    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3" style="text-align: left;">
      <h2>ACCOLADES</h2>
        {{#each educations as |education|}}
          <li>
            {{education.school}} | {{education.duration}}
          </li>
        {{/each}}
    </div>
    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2"></div>
  </div>
</div>
