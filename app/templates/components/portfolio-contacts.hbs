<div id="contacts">
  <!-- <h1>CONTACT ME</h1> -->
  <object id="Layer_6" type="image/svg+xml" data="contact.svg" style="height: 200px;"></object>
  <div class="container">
    <div class="row" style="margin-top: 50px;">
      <div class="col-lg-3"></div>
      <div class="col-lg-6">
        <form action="https://formspree.io/isabellaong@gmail.com" method="POST" id="contactForm" novalidate>
          <div class="row">
            <div class="form-group">
              <input name="name" type="text" class="form-control" placeholder="Your Name" id="name" required data-validation-required-message="Please enter your name.">
              <p class="help-block text-danger"></p>
            </div>
            <div class="form-group">
              <input name="email" type="email" class="form-control" placeholder="Your Email" id="email" required data-validation-required-message="Please enter your email address.">
              <p class="help-block text-danger"></p>
            </div>
            <div class="form-group">
              <input name="phone" type="tel" class="form-control" placeholder="Your Phone" id="phone" required data-validation-required-message="Please enter your phone number.">
              <p class="help-block text-danger"></p>
            </div>
            <br>
            <div class="form-group">
              <textarea name="message" class="form-control" placeholder="Your Message" rows="7" id="message" required data-validation-required-message="Please enter a message."></textarea>
              <p class="help-block text-danger"></p>
            </div>
            <div class="clearfix"></div>
            <div class="col-lg-12 text-center">
              <div id="success"></div>
              <button value="send" type="button submit" class="btn btn-xl" name="_next" value="http://www.isabellaong.me">Send Message</button>
            </div>
          </div>
        </form>
      </div>
      <div class="col-lg-1">
        {{#each links as |link|}}
        <a href="{{link.url}}"><img src="{{link.icon}}" style="width: 50px; display: flex; flex-direction: column; margin: 10px;"/></a>
        {{/each}}
      </div>
      <div class="col-lg-1"></div>
    </div>
  </div>
</div>
